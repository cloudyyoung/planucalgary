generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearchPostgres"]
}

generator zod {
  provider = "prisma-zod-generator"
  output   = "../node_modules/@prisma/zod"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["planucalgary", "catalog"]
}
