{
  "name": "@planucalgary/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared TypeScript types for planucalgary (client + server)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./prisma/client": {
      "import": "./dist/generated/prisma/client.js",
      "types": "./dist/generated/prisma/client.d.ts"
    },
    "./prisma/browser": {
      "import": "./dist/generated/prisma/browser.js",
      "types": "./dist/generated/prisma/browser.d.ts"
    }
  },
  "scripts": {
    "watch": "tsc -w -p tsconfig.json",
    "build": "rimraf tsconfig.tsbuildinfo && tsc -p tsconfig.json",
    "clean": "rimraf dist && rimraf node_modules && rimraf tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@types/express": "^5.0.6",
    "rimraf": "^6.1.2",
    "zod": "^4.1.13"
  }
}